    "express": "^4.17.3",
    "lodash": "^4.17.4",
    "method-override": "^2.3.10",
    "mongoose": "^4.13.4",
    "morgan": "^1.9.0",
    "redis": "^2.8.0",
    "request": "^2.83.0"
  }
