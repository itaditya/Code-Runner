    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.1",
    "css-loader": "^0.28.7",
    "extract-text-webpack-plugin": "^3.0.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.8.3",
    "prettier": "1.7.4",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^4.41.0",
    "webpack-cli": "^3.3.9"
  },
  "dependencies": {
    "ace": "^1.3.0",
